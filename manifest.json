{
	"manifest_version": 3,
	"name": "42 Projects Extension",
	"version": "1.0",
	"description": "Displays a list of projects from the 42 API.",
	"permissions": [
	  "storage",
	  "activeTab"
	],
	"background": {
	  "service_worker": "background.js"
	},
	"action": {
	  "default_popup": "hard-coding/popup.html",
	  "default_icon": "icons/icon.png" 
	},
	"content_scripts": [
	  {
		"matches": ["<all_urls>"],
		"js": ["content.js"]
	  }
	],
	"icons": {
	  "128": "icons/icon.png" 
	}
  }
  